<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login Page</title>
</head>
<body style="display: flex; justify-content: center; align-items: center; height: 100vh; background-color: #f0f0f0;">
    <div style="text-align: center;">
        <h2>Login</h2>
        <form class = "login-form" action="">
            <input type="text" placeholder="Username" id="usernameInput" style="padding: 10px; margin: 5px; border-radius: 5px; border: 1px solid #82A1D1;"><br>
            <input type="password" placeholder="Password" id="PasswordInput" style="padding: 10px; margin: 5px; border-radius: 5px; border: 1px solid #82A1D1;"><br>
            <button type="submit" id="loginButton" style="padding: 10px 20px; margin: 5px; border: none; border-radius: 5px; background-color: #82A1D1; color: white; cursor: pointer;">Login</button>
            <button type="button" id="signupButton" style="padding: 10px 20px; margin: 5px; border: none; border-radius: 5px; background-color: #82A1D1; color: white; cursor: pointer;">Sign Up</button>
        </form>
    </div>
</body>

<script src="./js/loginlogout.js"></script>